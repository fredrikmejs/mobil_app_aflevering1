<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Main_menu">
    <TextView
        android:text="Hovedmenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
        android:id="@+id/Hovedmenu_tekst" app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="40dp"
        app:layout_constraintHorizontal_bias="0.451" android:layout_marginBottom="59dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView"/>
    <ImageView
        android:layout_width="323dp"
        android:layout_height="0dp" app:srcCompat="@mipmap/ic_launcher"
        android:id="@+id/imageView" app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Hovedmenu_tekst" android:layout_marginBottom="288dp"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/aktivitet_1"
        app:layout_constraintStart_toStartOf="@+id/imageView" android:layout_marginTop="21dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView" android:text="web browser"/>
    <Button
        android:text="ny side 2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/aktivitet_2" android:layout_marginTop="21dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView" android:layout_marginEnd="27dp"
        android:layout_marginRight="27dp" app:layout_constraintEnd_toEndOf="@+id/imageView"/>
</androidx.constraintlayout.widget.ConstraintLayout>